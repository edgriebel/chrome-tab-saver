{
"name": "Tab Saver",
"version": "0.1",
"description": "save open tabs into folder",
"permissions": [
	"bookmarks",
	"tabs"
],
"browser_action": {
	"default_title": "Tab Saver",
	"default_icon": "icon.png",
	"default_popup": "popup.html"
},
"manifest_version": 2,
"content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
