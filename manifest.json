{
"name": "Tab Saver",
"version": "0.1",
"description": "save open tabs into folder",
"permissions": [
	"bookmarks",
	"tabs",
	"storage"
],
"options_page": "options.html",
"browser_action": {
	"default_title": "Tab Saver",
	"default_icon": "icon.png",
	"default_popup": "popup.html"
},
"icons": {
	"128": "icon.png"
},
"manifest_version": 2,
"content_security_policy": 
	"script-src 'self' https://maxcdn.bootstrapcdn.com https://ajax.googleapis.com; object-src 'self'"

}
